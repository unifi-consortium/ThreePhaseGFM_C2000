"1];\n"
"\n"
"    Comp->y1 = (Comp->yp1 + Comp->yi1[ki] + feedforward);\n"
"\n"
"    if(Comp->y1 > Comp->limits1.MaxValue){\n"
"\n"
"         Comp->xa1 = Comp->Ka1*(Comp->y1 - Comp->limits1.MaxValue);\n"
"         Comp->y1 = Comp->limits1.MaxValue;\n"
"\n"
"     }\n"
