  Script {
    Name          "Inductor Sweep"
    Script        "% create simStruct with field 'ModelVars'\nmdlVars = struct"
"('Lg', 50e-6);\nsimStruct = struct('ModelVars', mdlVars);\n\n% clear all prev"
"ious traces in scope 'Scope' in the current model\nplecs('scope', './Scope', "
"'ClearTraces');\nplecs('scope', './Scope3', 'ClearTraces');\nplecs('scope', '"
"./Scope5', 'ClearTraces');\n% parametric values to be swept\ninductorValues ="
" [0.1, 1, 10, 100]; % in mH\n\nfor ix = 1:length(inductorValues)\n  % set val"
"ue for L1\n  simStruct.ModelVars.Lg = inductorValues(ix) * 1e-3;\n  % start s"
"imulation, return probed signal values in 'out'  \n  out = plecs('simulate', "
"simStruct);\n  % hold and label trace\n  plecs('scope', './Scope', 'HoldTrace"
"', ['Lg=' mat2str(inductorValues(ix)) 'mH']);\n  plecs('scope', './Scope3', '"
"HoldTrace', ['Lg=' mat2str(inductorValues(ix)) 'mH']);\n  plecs('scope', './S"
"cope5', 'HoldTrace', ['Lg=' mat2str(inductorValues(ix)) 'mH']);\n  % find max"
"imum current value and index\nend"
  }
  Script {
    Name          "etaSweep"
    Script        "% create simStruct with field 'ModelVars'\nmdlVars = struct"
"('eta', 1);\nsimStruct = struct('ModelVars', mdlVars);\n\n% clear all previou"
"s traces in scope 'Scope' in the current model\nplecs('scope', './Scope', 'Cl"
"earTraces');\n\n% parametric values to be swept\ninductorValues = [0.1, 1, 10"
", 100]; % in mH\n\nfor ix = 1:length(inductorValues)\n  % set value for L1\n "
" simStruct.ModelVars.eta = inductorValues(ix);\n  % start simulation, return "
"probed signal values in 'out'  \n  out = plecs('simulate', simStruct);\n  % h"
"old and label trace\n  plecs('scope', './Scope', 'HoldTrace', ['eta=' mat2str"
"(inductorValues(ix))]);\n  % find maximum current value and index\nend"
  }
  Script {
    Name          "Script"
    Script        ""
  }
